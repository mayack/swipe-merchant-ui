{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "dom",
      "dom.iterable",
      "es6"
    ],
    "jsx": "react",
    "outDir": "./wwwroot",
    "rootDir": "./src",
    "importHelpers": false,
    "noEmitHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@app/*": [
        "src/app/*"
      ]
    },
    "types": [
      "node",
      "systemjs"
    ],
    "inlineSourceMap": true,
    "inlineSources": true
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
